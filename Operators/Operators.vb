Module Operators

    Sub Main()
        'example of '+' operator
        Console.Write("5 + 5 = ")
        Console.WriteLine(5 + 5)
        Console.WriteLine() 'add a blank line for reading
        'example'*' operator
        Console.Write("5 * 5 = ")
        Console.WriteLine(5 * 5)
        Console.WriteLine()
        'example '^' operator
        Console.Write("5 ^ 2 = ")
        Console.WriteLine(5 ^ 2)
        Console.WriteLine()
        'example of '/' operator. floating poit division
        Console.Write("5 / 2 = ")
        Console.WriteLine(5 / 2)
        Console.WriteLine()
        'example of '\' operator interger division
        Console.Write("5 \ 2 = ")
        Console.WriteLine(5 \ 2)
        Console.WriteLine()
        'example of 'Mod' operator.
        'remander from operator division
        Console.Write("5 Mod 2 = ")
        Console.WriteLine(5 Mod 2)
        Console.WriteLine()
        Console.Read() 'wait for user to respone 
    End Sub

End Module
